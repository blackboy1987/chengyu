.notice{
    background: rgba(116,94,192,.4);
    width:90%;
    margin: 22rpx auto;
    height: 50rpx;
    line-height: 50rpx;
    border-radius: 30rpx;
    color: #fff;
    display: flex;
}

.notice>.cuIcon-notification{
    margin-left: 20rpx;
    font-size: 32rpx;
    font-weight: 800;
}
.notice>.text{
    font-size: 28rpx;
    margin-left: 24rpx;
    color: #fff9ff;
}

.gameTitle{
    height: 260rpx;
    line-height: 260rpx;
    text-align: center;
    font-size: 64rpx;
}

@keyframes typing {
    from { width: 0; }
    to { width: 100%; }
}
@keyframes blink-caret {
    from, to { box-shadow: 2rpx 0 0 0 transparent; }
    50% { box-shadow: 2rpx 0 0 0; }
}

.userinfo{
    margin: 22rpx auto;
    background: rgba(116,94,192,.4);
    width:90%;
    display: flex;
    padding: 48rpx 24rpx;
    border-radius: 24rpx;
    align-content: center;
}
.userinfo>.header{
    border-radius: 100%;
    height: 140rpx;
    width: 140rpx;
    overflow: hidden;
}
.userinfo>.info{
    color: #FFF;
    font-size: 28rpx;
    padding-left: 48rpx;
    display: flex;
    justify-content: space-around;
    flex-direction: column;
}
.userinfo>.money{
    color: #FFF;
    padding-left: 48rpx;
    display: flex;
    justify-content: space-around;
    flex-direction: column;
}
.userinfo>.money>view{

}

.userinfo>.money .text{
    display: inline-block;
    padding-left: 24rpx;
    color:#FFF;
    font-size:28rpx;
    margin-top: -8rpx;
}
.btn{
    width: 90%;
    margin:0 auto;
}

.btn>button{
    border-radius: 60rpx;
    width: 100%;
    color: #FFF;
    font-size: 48rpx;
    padding:32rpx;
    background: #628ff6;
}

.tool{
    display: flex;
    justify-content: space-around;
}
.cu-list.grid{
    background: transparent !important;
}
.cu-list.grid>.cu-item text{
    margin-top: 32rpx;
    color: #fff7ff;
    font-size: 32rpx;
}


.no-login-modal>.cu-dialog{
    width: 80%;
}
.no-login-modal>.cu-dialog>.title{
    padding-top:48rpx;
    padding-bottom:36rpx;
    font-size: 36rpx;
    font-weight: 600;
}
.no-login-modal>.cu-dialog>.body{
    text-align: left;
    padding-left:64rpx;
}
.no-login-modal>.cu-dialog>.body>.title{
    height: 64rpx;
    line-height: 64rpx;
    margin-bottom: 24rpx;
}
.no-login-modal>.cu-dialog>.body>.text{
    height: 48rpx;
    line-height: 48rpx;
}
.no-login-modal>.cu-dialog>.footer{
    margin-top: 48rpx;
    display: flex;
    margin-bottom: 48rpx;
}
.no-login-modal>.cu-dialog>.footer>button{
    color: #fff;
    width: 240rpx;
    font-weight: normal;
    border-radius: 80rpx;
}
.no-login-modal>.cu-dialog>.footer>button.cancel{
    background: #bfbfbf;
}
.no-login-modal>.cu-dialog>.footer>button.ok{
    background: #5236af;
}

.rank .content1{
    background: #FFF;
}

.rank .content1 .item{
    display: flex;
    align-items: center;
    padding:8rpx 0;
    font-size: 32rpx;
}
.rank .content1 .item>.index{
    margin-right: 32rpx;

}
.rank .content1 .item>image{
    width: 100rpx;
    height: 100rpx;
    border-radius: 50%;
}
.rank .content1 .item>.name{
    width: 300rpx;
    padding-left: 16rpx;
    text-align: left;
}
.rank .content1 .item>.count{
    float: right;
}

.rank .btn{
    width: 100%;
}
.rank .btn>button{
    width: 100%;
    background:#5236af ;
    color: #FFF;
    font-weight: normal;
    border-radius: 30rpx;
    font-size: 28rpx;
}